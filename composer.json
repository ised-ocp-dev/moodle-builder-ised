    "scripts": {
        "post-install-cmd": [
            "mv .s2i dist/",
            "mv httpd-cfg dist/",
            "mv php-pre-start dist/",
            "mv dist/php-pre-start/config.php dist/",
            "mv dist/php-pre-start/defaults.php dist/local/",
            "mv dist/composer.* dist/php-pre-start/",
            "shopt -s dotglob && cp -r dist/* .",
            "rm -rf dist",
            "rm README.md",
            "ln -s /opt/app-root/src/vendor/ised-isde/remote-manage/rmanage.php rmanage.php",
            "git cherry-pick -n origin/MDL-63219-M39",
            "git cherry-pick -n origin/MDL-66856-M39",
            "git cherry-pick -n origin/MDL-67554-M39",
            "git cherry-pick -n origin/MDL-67802-M39",
            "git cherry-pick -n origin/MDL-52810-M39",
            "git cherry-pick -n origin/MDL-61880-M39",
            "git cherry-pick -n origin/MDL-64969-M39",
