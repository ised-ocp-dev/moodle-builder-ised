@Library('ised-cicd-lib') _

pipeline {
	agent {
		label 'php-7.3'
   	}

    options {
        disableConcurrentBuilds()
    }

   	environment {
		// GLobal Vars
		IMAGE_NAME = "dsdmdl-moodle"
		BUILD_NAME = "dsdmdl-moodle"
    }

    stages {
    	stage('release') {
			steps {
				script{
					release.releaseApp("${IMAGE_NAME}")
		    	}
			}
    	}
    }
}